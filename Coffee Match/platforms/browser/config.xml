<?xml version='1.0' encoding='utf-8'?>
<widget id="com.henrique.coffeematch" version="48.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Coffee Match</name>
    <description>Hello World sample application that responds to the deviceready event.</description>
    <author email="support@phonegap.com" href="http://phonegap.com">PhoneGap Team</author>
    <content src="index.html" />
    <gap:plugin name="onesignal-cordova-plugin" source="npm" spec="^2.0.10" />
    <config-file mode="replace" parent="NSPhotoLibraryUsageDescription" platform="ios">
        <string>Allows access to photo library</string>
    </config-file>
    <config-file mode="replace" parent="NSCameraUsageDescription" platform="ios">
        <string>Allows usage of the camera</string>
    </config-file>
    <icon src="img/logotipo.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="phonegap-version" value="cli-6.4.0" />
    <preference name="android-build-tool" value="gradle" />
    <preference name="windows-appx-target" value="8.1-phone" />
    <preference name="WindowsToastCapable" value="true" />
    <preference name="orientation" value="portrait" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
